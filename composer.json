{
  "name": "gsoares/bdd-php",
  "description": "Simple BDD use case using Behat for PHP",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "psr/http-message": "^1.0",
    "slim/slim": "^4.4.0",
    "ext-json": "*",
    "slim/psr7": "^1.0",
    "php-di/php-di": "^6.0",
    "ext-pdo": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Felipe Soares",
      "email": "gabrielfs7@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bdd\\": "src",
      "Bdd\\Test\\": "test/bdd"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "behat/behat": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.16"
  }
}
